{
  "devDependencies": {
    "@open-wc/building-rollup": "1.0.0",
    "@open-wc/eslint-config": "4.0.0",
    "@open-wc/testing": "2.5.32",
    "@open-wc/testing-karma": "3.0.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@types/chart.js": "^2.9.23",
    "@types/google.analytics": "0.0.40",
    "@types/hammerjs": "^2.0.36",
    "@types/i18next": "^13.0.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@types/socket.io-client": "^1.4.35",
    "@web/dev-server": "^0.1.8",
    "@web/test-runner": "^0.12.15",
    "es-dev-commonjs-transformer": "0.2.0",
    "es-dev-server": "1.5.0",
    "es-dev-server-rollup": "0.0.4",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "2.3.4",
    "ts-lit-plugin": "1.2.0",
    "tslib": "1.11.0",
    "typescript": "4.0.3"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "tsc && wtr --coverage",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start:build": "npm run build && web-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "tsc:watch": "tsc --watch",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index index.html --node-resolve --open --watch\""
  },
  "name": "cs-web-app",
  "version": "0.0.0",
  "description": "...",
  "author": "cs-web-app",
  "license": "LGPL",
  "dependencies": {
    "@kano/twemoji": "^1.0.0",
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-circular-progress-four-color": "^0.20.0",
    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-drawer": "^0.20.0",
    "@material/mwc-fab": "^0.20.0",
    "@material/mwc-formfield": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-icon-button": "^0.20.0",
    "@material/mwc-list": "^0.20.0",
    "@material/mwc-menu": "^0.20.0",
    "@material/mwc-radio": "^0.20.0",
    "@material/mwc-select": "^0.20.0",
    "@material/mwc-snackbar": "^0.20.0",
    "@material/mwc-tab-bar": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@material/mwc-top-app-bar": "^0.20.0",
    "@vaadin/vaadin-grid": "^6.0.1",
    "app-datepicker": "^4.1.0",
    "chart.js": "^2.9.3",
    "hammerjs": "^2.0.8",
    "i18next-http-backend": "^1.1.1",
    "koa-proxies": "^0.11.0",
    "linkifyjs": "^2.1.9",
    "lit-element": "^2.4.0",
    "lit-flexbox-literals": "0.0.7",
    "lit-html": "^1.3.0",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "socket.io-client": "^4.0.0",
    "vanilla-colorful": "^0.5.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "import/no-unresolved": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
